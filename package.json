{
  "name": "smartretaildb",
  "version": "1.0.0",
  "description": "SmartRetailDB is a SQL-based inventory and sales management system designed to streamline the operations of small to medium-sized supermarket chains.",
  "main": "index.js",
  "scripts": {
    "start": "npm --prefix client start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -f"
  },
  "homepage": "http://donshammah96.github.io/myapp",
  "dependencies": {
    "express": "^4.21.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^3.0.1",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "gh-pages": "^6.1.1"
  }
}
